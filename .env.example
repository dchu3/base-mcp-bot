GEMINI_API_KEY=your-gemini-key
GEMINI_MODEL=gemini-2.0-flash-lite
PLANNER_PROMPT_FILE=./prompts/planner.md
MCP_BASE_SERVER_CMD="node ../base-mcp-server/dist/index.js start"
MCP_DEXSCREENER_CMD="node /path/to/mcp-dexscreener/index.js"
MCP_HONEYPOT_CMD="bash -lc 'cd /path/to/base-mcp-honeypot && node dist/server.js stdio'"
MCP_WEBSEARCH_CMD="uvx duckduckgo-mcp-server"
MCP_DEXPAPRIKA_CMD="npx dexpaprika-mcp"
DEFAULT_LOOKBACK_MINUTES=30
MAX_ITEMS=20
DATABASE_URL=sqlite+aiosqlite:///./state.db
LOG_LEVEL=INFO

# Planner enhancements
PLANNER_CONFIDENCE_THRESHOLD=0.7
PLANNER_ENABLE_REFLECTION=true
PLANNER_MAX_ITERATIONS=2

# Planner mode: "simple" (pattern-based), "agentic" (LLM decides tools)
# Agentic mode is recommended for full functionality
PLANNER_MODE=agentic

# Agentic planner settings (used when PLANNER_MODE=agentic)
AGENTIC_MAX_ITERATIONS=8
AGENTIC_MAX_TOOL_CALLS=30
AGENTIC_TIMEOUT_SECONDS=90
